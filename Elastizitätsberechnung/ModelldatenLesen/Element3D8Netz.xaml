<Window x:Class="FE_Berechnungen.Elastizitätsberechnung.ModelldatenLesen.Element3D8Netz"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Elementnetz mit 8 Knoten in 3D definieren oder editieren" Height="220" Width="380">
    <Grid Height="200" VerticalAlignment="Center">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="160" />
            <ColumnDefinition Width="40" />
            <ColumnDefinition Width="120" />
            <ColumnDefinition Width="40" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="25" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <Label Name="Header" Grid.Row="0" Grid.ColumnSpan="4" Margin="0,0,-19,37" Grid.RowSpan="2">
            Präfix für Elemente und Knoten eingeben, Ids werden generiert
        </Label>

        <Label Grid.Row="1" Grid.Column="0" Margin="2,5,3,0">Element Präfix</Label>
        <TextBox Name="Elementpräfix" Grid.Row="1" Grid.Column="1" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Center" Width="32" />
        <Label Grid.Row="1" Grid.Column="1" Margin="37,5,38,0" Grid.ColumnSpan="3">Elementknoten Präfix</Label>
        <TextBox Name="Knotenpräfix" Grid.Row="1" Grid.Column="3" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Center" Width="32" />

        <Label Grid.Column="0" Grid.Row="2" Margin="1,5,0,6" HorizontalAlignment="Left" Width="160" Grid.ColumnSpan="3">
            Anzahl Elemente je Richtung
        </Label>
        <TextBox Name="Anzahl" Grid.Row="2" Grid.Column="1" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Center" Width="32" />

        <Label Grid.Column="0" Grid.Row="3" Margin="5,5,0,10" HorizontalAlignment="Left" Width="114">
            Elementmaterial Id
        </Label>
        <TextBox Name="Material" Grid.Row="3" Grid.Column="1" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Center" Width="32" />

        <Button Grid.Row="4" Grid.Column="3" Margin="4,5,6,16" Click="BtnDialogOk_Click">
            _Ok
        </Button>
        <Button Grid.Row="4" Grid.Column="2" Margin="8,5,5,16" Click="BtnDialogCancel_Click">
            _Abbrechen
        </Button>
    </Grid>
</Window>