<Window x:Class="FE_Berechnungen.Tragwerksberechnung.ModelldatenLesen.ElementNeu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Neues Element" Height="470" Width="320">
    <Grid Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150" />
            <ColumnDefinition Width="100" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="70" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="20" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.ColumnSpan="2" Margin="0,0,-66,0">
            Eingabewerte für ein neues oder vorhandenes Element
        </Label>
        <Label Grid.Column="0" Margin="0,23,0,10" Grid.RowSpan="2">Element ID:</Label>
        <Label Grid.Row="1" Grid.Column="0" Margin="0,9,0,1" FontSize="8">(generiert aus Start- und Endknoten ID)</Label>
        <TextBox Name="ElementId" Grid.Row="1" Grid.Column="1" Margin="10,0,-10,0" Height="19"
                 VerticalAlignment="Center" LostFocus="ElementIdLostFocus" />

        <Label Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,24,0,48" Grid.RowSpan="2">Elementtyp definieren:</Label>
        <CheckBox Name="FachwerkCheck" Grid.Row="2" Grid.Column="0" Margin="10,18,0,33"
                  Checked="FachwerkChecked">
            Fachwerk
        </CheckBox>
        <CheckBox Name="BalkenCheck" Grid.Row="2" Grid.Column="0" Margin="10,35,0,18"
                  Checked="BalkenChecked">
            Biegebalken
        </CheckBox>
        <CheckBox Name="FederCheck" Grid.Row="2" Grid.Column="0" Margin="10,52,0,1"
                  Checked="FederChecked">
            Federelement
        </CheckBox>
        <CheckBox Name="Gelenk1" Grid.Row="2" Grid.Column="1" Margin="11,17,-36,35">Gelenk am Anfang</CheckBox>
        <CheckBox Name="Gelenk2" Grid.Row="2" Grid.Column="1" Margin="11,35,-26,18">Gelenk am Ende</CheckBox>

        <Label Grid.Row="2" Grid.Column="0" Margin="0,64,0,13" Grid.RowSpan="2">Startknoten ID:</Label>
        <TextBox Name="StartknotenId" Grid.Row="3" Grid.Column="1" Margin="10,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="25" />
        <Label Grid.Row="3" Grid.Column="0" Margin="0,9,0,1" FontSize="8">(click vorhandene Knoten ID)</Label>
        <Label Grid.Row="3" Grid.Column="0" Margin="0,25,0,12" Grid.RowSpan="2">Endknoten ID:</Label>
        <TextBox Name="EndknotenId" Grid.Row="4" Grid.Column="1" Margin="10,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="25" />
        <Label Grid.Row="4" Grid.Column="0" Margin="0,9,0,1" FontSize="8">(click vorhandene Knoten ID)</Label>

        <Label Grid.Row="5" Grid.Column="0" Margin="0,0,-71,0" Grid.ColumnSpan="2" FontSize="8" FontWeight="Bold">click vorhandene Element Id für Übernahme von Material- und Querschnittswerten</Label>

        <Label Grid.Row="6" Grid.Column="0" Margin="0,0,0,0">Material ID:</Label>
        <TextBox Name="MaterialId" Grid.Row="6" Grid.Column="1" Margin="10,0,56,0" Height="20"
                 VerticalAlignment="Center" />

        <Label Grid.Row="7" Grid.Column="0" Margin="0,0,0,0">Elastizitätsmodul E:</Label>
        <TextBox Name="EModul" Grid.Row="7" Grid.Column="1" Margin="10,0,0,0" Height="20" VerticalAlignment="Center" />

        <Label Grid.Row="8" Grid.Column="0" Margin="0,0,0,0">spezifische Masse m:</Label>
        <TextBox Name="Masse" Grid.Row="8" Grid.Column="1" Margin="10,0,0,0" Height="20"
                 VerticalAlignment="Center" />
        <TextBox Name="Massen" Grid.Row="8" Grid.Column="1" Margin="10,0,0,0" Height="20" VerticalAlignment="Center" />

        <Label Grid.Row="9" Grid.Column="0" Margin="0,0,0,0">Querschnitt ID:</Label>
        <TextBox Name="QuerschnittId" Grid.Row="9" Grid.Column="1" Margin="10,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="34" />

        <Label Grid.Row="10" Grid.Column="0" Margin="0,0,0,0">Querschnittfläche A:</Label>
        <TextBox Name="Fläche" Grid.Row="10" Grid.Column="1" Margin="10,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="88" />

        <Label Grid.Row="11" Grid.Column="0" Margin="0,0,0,0">Trägheitsmoment I:</Label>
        <TextBox Name="Trägheitsmoment" Grid.Row="11" Grid.Column="1" Margin="10,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="89" />

        <Button Grid.Row="12" Grid.Column="0" Width="50" Height="26" Margin="5,0,0,0"
                Click="BtnLöschen_Click" HorizontalAlignment="Left" VerticalAlignment="Bottom">
            löschen
        </Button>
        <Button IsDefault="True" Grid.Row="12" Grid.Column="1" Width="35" Height="25" Margin="23,0,0,0"
                Click="BtnDialogOk_Click" HorizontalAlignment="Left" VerticalAlignment="Center">
            _Ok
        </Button>
        <Button Grid.Row="12" Grid.Column="1" Height="26" Margin="64,0,-35,0"
                Click="BtnDialogCancel_Click" VerticalAlignment="Center">
            _Abbrechen
        </Button>
    </Grid>
</Window>