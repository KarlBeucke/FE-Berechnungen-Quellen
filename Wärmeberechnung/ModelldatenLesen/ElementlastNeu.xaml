<Window x:Class="FE_Berechnungen.Wärmeberechnung.ModelldatenLesen.ElementlastNeu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Neue Elementlast" Height="400" Width="370">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200" />
            <ColumnDefinition Width="100" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="25" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <Label Name="Header" Grid.Row="0" Grid.ColumnSpan="2" Margin="0,0,-0.4,30.2" Grid.RowSpan="2">
            Definition einer neuen Elementlast
        </Label>

        <Label Grid.Row="1" Grid.Column="0" Margin="0,5,0,0">Elementlast ID:</Label>
        <TextBox Name="ElementlastId" Grid.Row="1" Grid.Column="1" Margin="0,0,-11,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Right" Width="101" LostFocus="ElementlastIdLostFocus" />
        <Label Grid.Row="2" Grid.Column="0" Margin="0,5,0,0">Element ID:</Label>
        <TextBox Name="ElementId" Grid.Row="2" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="3" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 1:
        </Label>
        <TextBox Name="Knoten1" Grid.Row="3" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="4" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 2:
        </Label>
        <TextBox Name="Knoten2" Grid.Row="4" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="5" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 3:
        </Label>
        <TextBox Name="Knoten3" Grid.Row="5" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="6" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 4:
        </Label>
        <TextBox Name="Knoten4" Grid.Row="6" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="7" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 5:
        </Label>
        <TextBox Name="Knoten5" Grid.Row="7" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="8" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 6:
        </Label>
        <TextBox Name="Knoten6" Grid.Row="8" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="9" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 7:
        </Label>
        <TextBox Name="Knoten7" Grid.Row="9" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />
        <Label Grid.Row="10" Grid.Column="0" Margin="0,0,0,0" Width="199"
               HorizontalAlignment="Left" VerticalAlignment="Center">
            Knotentemperatur am Knoten 8:
        </Label>
        <TextBox Name="Knoten8" Grid.Row="10" Grid.Column="1" Margin="12.2,0,0,0" Height="20"
                 VerticalAlignment="Center" HorizontalAlignment="Left" Width="32" />

        <Button Grid.Row="11" Grid.Column="0" Width="50" Height="26" Margin="5,0,0,0"
                Click="BtnLöschen_Click" HorizontalAlignment="Left" VerticalAlignment="Bottom">
            löschen
        </Button>
        <Button IsDefault="True" Grid.Row="11" Grid.Column="1" Height="24" Margin="0,4.6,-45.4,0"
                Click="BtnDialogOk_Click" VerticalAlignment="Top" HorizontalAlignment="Right" Width="39">
            _Ok
        </Button>
        <Button Grid.Row="11" Width="75" Height="26" Margin="0,0,-1.4,-0.2"
                Click="BtnDialogCancel_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Grid.Column="1">
            _Abbrechen
        </Button>
    </Grid>
</Window>